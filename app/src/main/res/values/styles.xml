<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavigation.ActiveItemTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_10ssp</item>
    </style>

    <style name="TitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/h9A000000</item>
    </style>

    <style name="TitleStyle.titleNormal">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_14ssp</item>
    </style>

    <style name="TitleStyle.edittext">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textStyle">normal</item>
    </style>
</resources>